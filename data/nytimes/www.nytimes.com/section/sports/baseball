=\"tabContent active\"\u003E \u003C\u002Fdiv\u003E\r\n\u003C\u002Fdiv\u003E\r\n\r\n\u003Cscript type=\"text\u002Fjavascript\" src=\"https:\u002F\u002Fstatic01.nyt.com\u002Fjs\u002Fapp\u002Flib\u002Fjquery\u002Fjquery-1.7.1.min.js\"\u003E\u003C\u002Fscript\u003E\r\n\r\n\u003Cscript type=\"text\u002Fjavascript\"\u003E\r\n    var NYTD = NYTD || {};\r\n\r\n    NYTD.TabSet =function() {\r\n\r\n        var $statsWidget = $('#sportsStatsWidget');\r\n\r\n        var api = {\r\n            selectedTabNotFound: function() { return $statsWidget.find(\"ul.tabs li.selected\").length \u003C 1},\r\n            activeContentNotFound: function() { return $statsWidget.find(\".tabContent.active\").length \u003C 1},\r\n            getTabs: function() { return $statsWidget.find(\"ul.tabs li\")},\r\n            getTabContent: function() { return $statsWidget.find(\".tabContent\")},\r\n            activateTab: function(tabNode, contentNode) {activateTab(tabNode, contentNode);},\r\n        };\r\n\r\n        function activateTabLink(e) {\r\n            var $el = $(e.currentTarget);\r\n            blankTabsAndContent();\r\n            selectTab($el.closest('li'));\r\n        }\r\n\r\n        function blankTabsAndContent() {\r\n            api.getTabs().removeClass('selected');\r\n            api.getTabContent().removeClass('active');\r\n        }\r\n\r\n        function selectTab(tabNode) {\r\n            if (tabNode) {\r\n              var index = api.getTabs().index(tabNode);\r\n              activateTab(tabNode, api.getTabContent().eq(index));\r\n            }\r\n        }\r\n\r\n        function activateTab(tabNode, contentNode) {\r\n            if (tabNode && contentNode) {\r\n                tabNode.addClass(\"selected\");\r\n                contentNode.addClass(\"active\");\r\n            }\r\n        }\r\n\r\n        $statsWidget.on('click', 'ul.tabs li', activateTabLink)\r\n\r\n        if(api.selectedTabNotFound() || api.activeContentNotFound()) {\r\n            blankTabsAndContent();\r\n            activateTab(api.getTabs().first(), api.getTabContent().first());\r\n        }\r\n\r\n        return api;\r\n    };\r\n\r\n    \u002F\u002F Automatically tab-i-fy nodes with a class of tabAutoLoad\r\n    $(document).ready(function(){\r\n        NYTD.TabSet();\r\n        $(\".shsTableTtlRow td a\")[0].href = \"https:\u002F\u002Fwww.nytimes.com\u002Fsports\u002Fmlb\u002Fscoreboard\"\r\n    });\r\n\u003C\u002Fscript\u003E\r\n\r\n\u003Cscript src=\"https:\u002F\u002Fnytimes.stats.com\u002Ftoday.js.asp?target_id=statscontent&sport=mlb\"\u003E\u003C\u002Fscript\u003E\r\n","compatibility":"NYT5","headline":{"type":"id","generated":true,"id":"$EmbeddedInteractive:RW1iZWRkZWRJbnRlcmFjdGl2ZTpueXQ6Ly9lbWJlZGRlZGludGVyYWN0aXZlLzE5M2QyOThhLWE3YjYtNTdkOS1iZGE3LThjN2Y2ZDA0MGNlNw==.headline","typename":"CreativeWorkHeadline"},"advertisingProperties":{"type":"id","generated":true,"id":"$EmbeddedInteractive:RW1iZWRkZWRJbnRlcmFjdGl2ZTpueXQ6Ly9lbWJlZGRlZGludGVyYWN0aXZlLzE5M2QyOThhLWE3YjYtNTdkOS1iZGE3LThjN2Y2ZDA0MGNlNw==.advertisingProperties","typename":"CreativeWorkAdvertisingProperties"},"displayProperties":{"type":"id","generated":true,"id":"$EmbeddedInteractive:RW1iZWRkZWRJbnRlcmFjdGl2ZTpueXQ6Ly9lbWJlZGRlZGludGVyYWN0aXZlLzE5M2QyOThhLWE3YjYtNTdkOS1iZGE3LThjN2Y2ZDA0MGNlNw==.displayProperties","typename":"CreativeWorkDisplayProperties"},"bylines":[],"sourceId":"100000004701974","credit":"","leadin":"","note":"","dataSource":"","uri":"nyt:\u002F\u002Fembeddedinteractive\u002F193d298a-a7b6-57d9-bda7-8c7f6d040ce7","url":"","firstPublished":"2016-10-10T23:30:08.000Z","summary":"","kicker":"","column":null,"promotionalMedia":null},"$EmbeddedInteractive:RW1iZWRkZWRJbnRlcmFjdGl2ZTpueXQ6Ly9lbWJlZGRlZGludGVyYWN0aXZlLzE5M2QyOThhLWE3YjYtNTdkOS1iZGE3LThjN2Y2ZDA0MGNlNw==.headline":{"default":"Stats and Schedules","__typename":"CreativeWorkHeadline"},"$EmbeddedInteractive:RW1iZWRkZWRJbnRlcmFjdGl2ZTpueXQ6Ly9lbWJlZGRlZGludGVyYWN0aXZlLzE5M2QyOThhLWE3YjYtNTdkOS1iZGE3LThjN2Y2ZDA0MGNlNw=