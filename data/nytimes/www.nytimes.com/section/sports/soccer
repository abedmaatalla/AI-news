<script type="text/javascript" src="https://static01.nyt.com/js/app/lib/jquery/jquery-1.7.1.min.js"></script>

<script type="text/javascript">
    var NYTD = NYTD || {};

    NYTD.TabSet =function() {

        var $statsWidget = $('#sportsStatsWidget');

        var api = {
            selectedTabNotFound: function() { return $statsWidget.find("ul.tabs li.selected").length < 1},
            activeContentNotFound: function() { return $statsWidget.find(".tabContent.active").length < 1},
            getTabs: function() { return $statsWidget.find("ul.tabs li")},
            getTabContent: function() { return $statsWidget.find(".tabContent")},
            activateTab: function(tabNode, contentNode) {activateTab(tabNode, contentNode);},
        };

        function activateTabLink(e) {
            var $el = $(e.target);
            blankTabsAndContent();
            selectTab($el.parent().selector[0]);

        }

        function blankTabsAndContent() {
            api.getTabs().removeClass('selected');
            api.getTabContent().removeClass('active');
        }

        function selectTab(tabNode) {
            if (tabNode) {
              var index = api.getTabs().index(tabNode);
              activateTab($(tabNode), api.getTabContent().eq(index));
            }
        }

        function activateTab(tabNode, contentNode) {
            if (tabNode && contentNode) {
                tabNode.addClass("selected");
                contentNode.addClass("active");
            }
        }

        $statsWidget.on('click', 'ul.tabs li a', activateTabLink)

        if(api.selectedTabNotFound() || api.activeContentNotFound()) {
            blankTabsAndContent();
            activateTab(api.getTabs().first(), api.getTabContent().first());
        }

        return api;
    };

    // Automatically tab-i-fy nodes with a class of tabAutoLoad
    $(document).ready(function(){
        NYTD.TabSet();
        $(".shsTableTtlRow td a")[0].href = "https://nytimes.stats.com/worldcup/scoreboard_daily.asp"
    });
</script>

<script src="https://nytimes.stats.com/today.js.asp?target_id=statscontent&sport=IFB:WORLDCUP|IFB:EPL|IFB:MLS|IFB:CHLG|IFB:SERI|IFB:LIGA"></script>
                    </div>
