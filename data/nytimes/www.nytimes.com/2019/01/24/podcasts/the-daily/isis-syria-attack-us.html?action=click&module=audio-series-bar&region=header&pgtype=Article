    <script>
      
(function (w, d, fontManifest, needed, rest) {
  var supportsWoff2 = function() {
    if (!('FontFace' in w)) return false;
    var f = new FontFace('t', 'url("data:application/font-woff2;base64,d09GMgABAAAAAADcAAoAAAAAAggAAACWAAEAAAAAAAAAAAAAAAAAAAAAAAAAAAAABk4ALAoUNAE2AiQDCAsGAAQgBSAHIBtvAcieB3aD8wURQ+TZazbRE9HvF5vde4KCYGhiCgq/NKPF0i6UIsZynbP+Xi9Ng+XLbNlmNz/xIBBqq61FIQRJhC/+QA/08PJQJ3sK5TZFMlWzC/iK5GUN40psgqvxwBjBOg6JUSJ7ewyKE2AAaXZrfUB4v+hze37ugJ9d+DeYqiDwVgCawviwVFGnuttkLqIMGivmDg") format("woff2")', {});
    f.load()['catch'](function() {});
    return f.status == 'loading' || f.status == 'loaded';
  }

  var addFontFace = function(fontFace) {
    var style = d.createElement('style');
    style.appendChild(d.createTextNode(fontFace));
    d.querySelector('head').appendChild(style);
  };

  var fetchFontFace = function(url, format, hash, hashName, fontName) {
    var xhr = new XMLHttpRequest();
    xhr.open('GET', url, true);
    xhr.onreadystatechange = function() {
      if (xhr.readyState === 4 && xhr.status === 200) {
        addFontFace(xhr.responseText);
        try {
          localStorage.setItem('nyt-fontFormat', format);
          localStorage.setItem(hashName, hash);
        } catch(e) { return; }
        localStorage.setItem(fontName, xhr.responseText);
      }
      return true;
    };
    xhr.send(null);
  };

  var loadFonts = function(bundles, now) {
    var fontFormat;
    try {
      fontFormat = localStorage.getItem('nyt-fontFormat');
    } catch(e) {}
    if (!fontFormat) fontFormat = supportsWoff2() ? 'woff2' : 'woff';

    for (var i = 0; i < bundles.length; i++) {
      var bundle = bundles[i];
      var ext = bundle !== 'shared' ? '-' + bundle : '';
      var hashName = 'nyt-fontHash' + ext;
      var fontName = 'nyt-fontFace' + ext;
      var font = fontManifest[bundle][fontFormat];
      var oldHash = localStorage.getItem(hashName);
      var fontFace = localStorage.getItem(fontName);
      if (oldHash === font.hash && fontFace) {
        addFontFace(fontFace);
      } else {
        var load = function(url, format, hash, hashName, fontName) {
          return function() {
            fetchFontFace(url, format, hash, hashName, fontName);
          }
        }(font.url, fontFormat, font.hash, hashName, fontName);
        if (now) load();
        else document.addEventListener('DOMContentLoaded', load);
      }
    }
  };
  loadFonts(needed);
  window.addEventListener('load', function() { loadFonts(rest, true); });
