	#g-0203-tra-webAFRICACYCLINGmap-300 .g-pstyle10 {
		font-style:italic;
		font-size:12px;
		line-height:13px;
		height:13px;
		mix-blend-mode:multiply;
		letter-spacing:0.025em;
		text-align:center;
		color:rgb(100,135,170);
	}

</style>

