<script>
    (function() {
        /* Smartlock Logout */
        if (window.navigator.credentials) {
            if (window.navigator.credentials.preventSilentAccess) {
                window.navigator.credentials.preventSilentAccess();
            } else {
                window.navigator.credentials.requireUserMediation();
            }
        }
    })();
</script>


