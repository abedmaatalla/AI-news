<script>
  var _gaq = _gaq || [];
  define('_nytg/2019-01-02-trump-border-wall/assets', function() { return 'https://static01.nyt.com/newsgraphics/2019/01/02/trump-border-wall/8c94200a5cbdf3b9521b404099da0196afdc47ac/'; });
