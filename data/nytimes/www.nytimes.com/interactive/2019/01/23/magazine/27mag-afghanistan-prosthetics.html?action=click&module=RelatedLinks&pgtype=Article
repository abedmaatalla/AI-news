  <div class="rad-media-wrapper">
    <div class="image" style="background-color: #dedad7">
      <div class="rad-spinner"></div>
    
      <img class="rad-lazy media-viewer-candidate"
        src='data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7'
        style='height: 0 !important; padding-bottom: 133.33333333333331%'
        data-pattern='https://static01.nyt.com/images/2019/01/27/magazine/27mag-prosthetics-NEW-slide-VTBK/{{file}}'
        data-widths='{"master":[{"size":180,"filename":"27mag-prosthetics-NEW-slide-VTBK-master180.png"},{"size":495,"filename":"27mag-prosthetics-NEW-slide-VTBK-master495.png"},{"size":675,"filename":"27mag-prosthetics-NEW-slide-VTBK-master675.png"},{"size":768,"filename":"27mag-prosthetics-NEW-slide-VTBK-jumbo.png"},{"size":1536,"filename":"27mag-prosthetics-NEW-slide-VTBK-superJumbo.png"}],"square":[{"size":75,"filename":"27mag-prosthetics-NEW-slide-VTBK-thumbStandard.png"},{"size":150,"filename":"27mag-prosthetics-NEW-slide-VTBK-thumbLarge.png"},{"size":320,"filename":"27mag-prosthetics-NEW-slide-VTBK-square320.png"},{"size":640,"filename":"27mag-prosthetics-NEW-slide-VTBK-square640.png"}],"threeTwo":[{"size":75,"filename":"27mag-prosthetics-NEW-slide-VTBK-videoThumb.png"},{"size":190,"filename":"27mag-prosthetics-NEW-slide-VTBK-thumbWide.png"},{"size":225,"filename":"27mag-prosthetics-NEW-slide-VTBK-mediumThreeByTwo225.png"},{"size":768,"filename":"27mag-prosthetics-NEW-slide-VTBK-videoLarge.png"}]}'
        data-master-ratio='1.3333333333333333'
      
        data-mediaviewer-src='https://static01.nyt.com/images/2019/01/27/magazine/27mag-prosthetics-NEW-slide-VTBK/27mag-prosthetics-NEW-slide-VTBK-superJumbo.png'
