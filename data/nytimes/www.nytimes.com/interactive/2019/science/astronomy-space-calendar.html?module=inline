<script>
  // Ad handling
  require(['foundation/main'], function() {
    require([
      'interactive/instances/ad-manager',
      'jquery/nyt'
      ], function(dfp, $) {
        try {
            var small = !$('html').hasClass('viewport-medium-30');
            var size = small ? [300, 250] : [728, 90];
            var placements = {};

            $('.g-ad-wrapper').each(function(i, el) {

                var id = small ? 'MiddleRight' + (i + 1) : 'TopAd' + (i + 1);

                // top-ad appears to be rules NYT5 uses for basic styling.
                // the other classes are for our use...
                var c = small ? "g-middleright-ad" : "g-top-ad top-ad";

                placements[id] = size;
                $(el).append('<div id="' + id + '" class="ad g-ad ' + c + '"></div>');
            });

            dfp.addPlacement(placements);
            dfp.scan();

        } catch(e) {
          console.log('Error loading ads', e)
        } // End try catch
    })
  });
</script>
